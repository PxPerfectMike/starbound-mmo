{
  "name": "starbound-mmo",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,svelte,js,json,md}\"",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:studio": "pnpm --filter @starbound-mmo/db studio"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": "20.x"
  }
}
